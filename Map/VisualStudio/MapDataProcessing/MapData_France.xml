<?xml version="1.0" encoding="utf-8"?>
<mapData xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="XmlMapData.xsd">

  <parameters logFileName="log.txt" fileLogLevel="MESSAGE" consoleLogLevel="MESSAGE" databaseName="diaphneaMap"
              kmlDir="../../../EditableKml_Input" maxConnectionDistanceInKm="1.0"/>
    
  <resolutionList>
    <resolution outputDir="_250m" sampleLength1="0.05" sampleRatio="5" smoothingWidth="15" />
    <resolution outputDir="_500m" sampleLength1="0.1" sampleRatio="5" smoothingWidth="14" />
    <resolution outputDir="_1km" sampleLength1="0.1" sampleRatio="10" smoothingWidth="24" />
    <resolution outputDir="_2km" sampleLength1="0.1" sampleRatio="20" smoothingWidth="40" />
    <resolution outputDir="_4km" sampleLength1="0.1" sampleRatio="40" smoothingWidth="64" />
  </resolutionList>

  <lookList>
    <polygonLookList>
      <polygonLook id="pl_France"/>
      <polygonLook id="pl_Region"/>
      <polygonLook id="pl_Departement"/>
    </polygonLookList>
    <lineLookList>
      <lineLook id="ll_FleuveRiviere"/>
    </lineLookList>
    <pointLookList>
      <pointLook id="tl_Prefecture"/>
      <pointLook id="tl_SousPrefecture"/>
      <pointLook id="tl_Ville"/>
    </pointLookList>
  </lookList>
  
