      <polygonElement id="e_Bretagne" name="Bretagne" look="pl_Region"/>
      <polygonElement id="e_CVL" name="Centre-Val de Loire" look="pl_Region"/>
      <polygonElement id="e_Corse" name="Corse" look="pl_Region"/>
      <polygonElement id="e_Est" name="Grand-Est" look="pl_Region"/>
      <polygonElement id="e_HDF" name="Hauts-de-France" look="pl_Region"/>
      <polygonElement id="e_IDF" name="Île-de-France" look="pl_Region"/>
      <polygonElement id="e_Normandie" name="Normandie" look="pl_Region"/>
      <polygonElement id="e_Aquitaine" name="Nouvelle-Aquitaine" look="pl_Region"/>
