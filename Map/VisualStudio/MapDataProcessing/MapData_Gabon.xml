<?xml version="1.0" encoding="utf-8"?>
<mapData xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="XmlMapData.xsd">

  <parameters mapId="_Gabon" logFileName="log.txt" fileLogLevel="MESSAGE" consoleLogLevel="MESSAGE" databaseName="diaphnea"
              kmlDir="../../../EditableKml_Input" maxConnectionDistanceInKm="5.0">
    <languageList>
      <language id="fr" name="Français"/>
      <language id="en" name="English"/>
    </languageList>
  </parameters>

  <resolutionList>
    <resolution outputDir="../../../InfoKml_Output/Gabon/_250m" sampleLength1="0.05" sampleRatio="5" smoothingWidth="15" />
    <resolution outputDir="../../../InfoKml_Output/Gabon/_500m" sampleLength1="0.1" sampleRatio="5" smoothingWidth="14" />
    <resolution outputDir="../../../InfoKml_Output/Gabon/_1km" sampleLength1="0.1" sampleRatio="10" smoothingWidth="24" />
    <resolution outputDir="../../../InfoKml_Output/Gabon/_2km" sampleLength1="0.1" sampleRatio="20" smoothingWidth="40" />
    <resolution outputDir="../../../InfoKml_Output/Gabon/_4km" sampleLength1="0.1" sampleRatio="40" smoothingWidth="64" />
  </resolutionList>

  <lookList>
    <polygonLookList>
      <polygonLook id="pl_Gabon"/>
      <polygonLook id="pl_Province"/>
      <polygonLook id="pl_Departement"/>
    </polygonLookList>
    <lineLookList>
      <lineLook id="ll_FleuveRiviere"/>
    </lineLookList>
    <pointLookList>
      <pointLook id="tl_ChefLieuProvince"/>
      <pointLook id="tl_ChefLieuDepartement"/>
      <pointLook id="tl_Ville"/>
    </pointLookList>
  </lookList>

  <elementList>
    <polygonElementList>
      <!--<polygonElement id="e_Gabon" look="pl_Gabon">
        <name>
          <name language="fr" text=""/>
          <name language="en" text=""/>
        </name>
      </polygonElement>-->
      <!--Province-->
      <polygonElement id="e_Estuaire" look="pl_Province">
        <name>
          <name language="fr" text="Estuaire (1)"/>
          <name language="en" text="Estuaire (1)"/>
        </name>
        <shortName>
          <name language="fr" text="1"/>
          <name language="en" text="1"/>
        </shortName>
      </polygonElement>
      <!--Départements-->
      <polygonElement id="e_Noya" look="pl_Departement">
        <name>
          <name language="fr" text="Noya"/>
          <name language="en" text="Noya"/>
        </name>
      </polygonElement>
      <polygonElement id="e_KomoMondah" look="pl_Departement">
        <name>
          <name language="fr" text="Komo-Mondah"/>
          <name language="en" text="Komo-Mondah"/>
        </name>
      </polygonElement>
      <polygonElement id="e_KomoOcean" look="pl_Departement">
        <name>
          <name language="fr" text="Komo-Océan"/>
          <name language="en" text="Komo-Océan"/>
        </name>
      </polygonElement>
      <polygonElement id="e_LibrevilleDepartement" look="pl_Departement">
        <name>
          <name language="fr" text="Libreville"/>
          <name language="en" text="Libreville"/>
        </name>
      </polygonElement>
      <polygonElement id="e_Komo" look="pl_Departement">
        <name>
          <name language="fr" text="Komo"/>
          <name language="en" text="Komo"/>
        </name>
      </polygonElement>
    </polygonElementList>
    <lineElementList/>
    <pointElementList/>
  </elementList>

</mapData>

